{
  "name": "acn-migrate-mongo",
  "version": "1.1.0",
  "description": "A database migration tool for MongoDB in Node, based on the migrate-mongo module",
  "main": "lib/migrate-mongo.js",
  "bin": {
    "migrate-mongo": "bin/migrate-mongo.js"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --recursive",
    "test-coverage": "nyc --reporter=text-lcov mocha --recursive | coveralls",
    "lint": "eslint lib/ test/"
  },
  "author": "Pietro Paolo Ferrari",
  "license": "MIT",
  "keywords": [
    "migrate mongo mongodb migrations database"
  ],
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "dependencies": {
    "cli-table": "0.3.1",
    "commander": "3.0.0",
    "date-fns": "2.0.1",
    "fn-args": "5.0.0",
    "fs-extra": "8.1.0",
    "lodash": "4.17.15",
    "mongodb": "3.3.1",
    "p-each-series": "2.1.0",
    "hasha": "5.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.6",
    "eslint": "6.2.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-mocha": "6.1.0",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "proxyquire": "2.1.3",
    "sinon": "7.4.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  }
}
